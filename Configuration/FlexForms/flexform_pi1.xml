<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>OSM Settings</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.mapUid>
                        <TCEforms>
                            <label>Select OSM Map</label>
                            <config>
                                <type>select</type>
                                <foreign_table>tx_pbosmpartners_domain_model_map</foreign_table>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">-- Choose --</numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.mapUid>
                    <settings.mapWidth>
                        <TCEforms>
                            <label>Map Width</label>
                            <config>
                                <type>input</type>
                                <default>100%</default>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </settings.mapWidth>
                    <settings.mapHeight>
                        <TCEforms>
                            <label>Map Height</label>
                            <config>
                                <type>input</type>
                                <default>550px</default>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </settings.mapHeight>
                    <settings.initZoomLevel>
                        <TCEforms>
                            <label>Initial Zoom Level</label>
                            <config>
                                <type>input</type>
                                <default>6</default>
                                <size>30</size>
                                <eval>int</eval>
                            </config>
                        </TCEforms>
                    </settings.initZoomLevel>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
